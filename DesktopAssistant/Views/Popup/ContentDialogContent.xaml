<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="DesktopAssistant.Views.Popup.ContentDialogContent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:DesktopAssistant.Views.Popup"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <StackPanel
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        Background="White">
        <TextBlock x:Uid="Dialog_Content1" TextWrapping="Wrap" />
        <TextBlock x:Uid="Dialog_Content2" TextWrapping="Wrap" />
    </StackPanel>
</Page>
